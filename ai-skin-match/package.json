{
  "name": "directus-extension-ai-skin-match",
  "icon": "extension",
  "version": "1.5.3",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-bundle"
  ],
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "api": "dist/api.js",
      "app": "dist/app.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "recommend",
        "source": "src/recommend/index.js"
      },
      {
        "type": "endpoint",
        "name": "shop",
        "source": "src/shop/index.js"
      },
      {
        "type": "hook",
        "name": "auto-update-product-status",
        "source": "src/auto-update-product-status/index.js"
      },
      {
        "type": "hook",
        "name": "auto-retrain",
        "source": "src/auto-retrain/index.js"
      },
      {
        "type": "module",
        "name": "ai-management",
        "source": "src/ai-management/index.js"
      },
      {
        "type": "module",
        "name": "product-management",
        "source": "src/product-management/index.js"
      },
      {
        "type": "hook",
        "name": "ingredient-processor",
        "source": "src/ingredient-processor/index.js"
      }
    ],
    "host": "^9.26.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^9.26.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "sass": "^1.94.0",
    "typescript": "^5.9.3",
    "vue": "^3.4.38"
  },
  "dependencies": {
    "@directus/composables": "^9.26.0",
    "@directus/errors": "^1.0.0",
    "@formkit/vue": "^0.17.3",
    "axios": "^1.13.2",
    "papaparse": "^5.5.3",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
