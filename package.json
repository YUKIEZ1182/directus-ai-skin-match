{
  "name": "directus-ai-skin-match",
  "description": "Custom extensions for the AI Skin Match project",
  "version": "0.1.0",
  "files": [
    "dist"
  ],
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "start": "npx directus start"
  },
  "dependencies": {
    "directus": "9.26.0",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "9.26.0"
  },
  "directus:extension": {
    "host": "^9.26.0",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "mock-api",
        "source": "src/mock-api/index.js"
      }
    ]
  }
}
